{
  "name": "jseditor",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "android": "react-native run-android",
    "android:release": "react-native run-android --variant=release",
    "android:clean": "cd android && ./gradlew clean && cd ..",
    "ios": "react-native run-ios",
    "build:debug": "cd android && ./gradlew assembleDebug && cd ..",
    "build:release": "cd android && ./gradlew assembleRelease && cd ..",
    "bundle:release": "cd android && ./gradlew bundleRelease && cd ..",
    "test": "jest",
    "detox": "detox test -c android.emu.debug",
    "detox:reuse": "detox test -c android.emu.debug --reuse",
    "detox:release": "detox test -c android.emu.release",
    "android:logs": "adb logcat"
  },
  "dependencies": {
    "@react-native-community/async-storage": "1.5.0",
    "easy-peasy": "3.0.0",
    "i18n-js": "3.3.0",
    "react": "16.8.3",
    "react-native": "0.59.9",
    "react-native-drawer": "2.5.1",
    "react-native-fs": "2.13.3",
    "react-native-gesture-handler": "1.3.0",
    "react-native-options-menu": "2.0.2",
    "react-native-reanimated": "1.1.0",
    "react-native-syntax-highlighter": "2.0.1",
    "react-native-tab-view": "2.9.0",
    "react-native-vector-icons": "6.5.0",
    "react-navigation": "3.11.0",
    "react-navigation-transitions": "1.0.10"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/runtime": "^7.4.5",
    "babel-jest": "^24.8.0",
    "detox": "^14.0.1",
    "jest": "^24.8.0",
    "metro-react-native-babel-preset": "^0.54.1",
    "mocha": "^6.2.0",
    "react-test-renderer": "16.8.3"
  },
  "jest": {
    "preset": "react-native"
  },
  "detox": {
    "configurations": {
      "android.emu.debug": {
        "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
        "build": "cd android ; ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug ; cd -",
        "type": "android.emulator",
        "name": "Pixel_2_API_28"
      },
      "android.emu.release": {
        "binaryPath": "android/app/build/outputs/apk/release/app-release.apk",
        "build": "cd android ; ./gradlew assembleRelease assembleAndroidTest -DtestBuildType=release ; cd -",
        "type": "android.emulator",
        "name": "Pixel_2_API_28"
      }
    },
    "test-runner": "jest"
  }
}
